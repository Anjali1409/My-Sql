create database dd;
use dd;
create table employee(ID int NOT NULL,NAME varchar(15) NOT NULL,AGE int NOT NULL,ADDRESS varchar(50) NOT NULL,SALARY float NOT NULL,primary key(ID));
create table e1(e_id int,salary float,foreign key(e_id) references employee(ID));
desc e1;
insert into employee values(1,'Ramesh',32,'Ahmedabad',2000);
