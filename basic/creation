create database my_sql;
create table gg(
  id INT NOT NULL,
  f_name VARCHAR(15) NOT NULL,
  l_name VARCHAR(15) NOT NULL,
  dob INT NULL,
  dept VARCHAR(10) NOT NULL,
  address VARCHAR(50) NOT NULL,
  city VARCHAR(20) NOT NULL,
  state VARCHAR(20) NOT NULL,
  pin INT NULL,
  phone INT NOT NULL
  );
insert into gg values(1,'aa','ss',NULL,'cse','old T.V.S. road,theni','Theni','Tamil Nadu',655023);
select id,city from gg;
  
